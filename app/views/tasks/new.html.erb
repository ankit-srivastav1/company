<h1>New Task</h1>

<%= render 'form', task: @task %>

<%= link_to 'Back', tasks_path %>


<div class="row">
  <div class="col-md-9 col-md-offset-3">
    <%= form_for(@task) do |f| %>

    <%= f.label :tname %>
    <%= f.text_field :tname, class: 'form-control' %>

    <%= f.label :assigned_by%>
    <%= f.text_field :assigned_by, class: 'form-control' %>

    <%= f.label :project_id%>
    <%= f.text_field :project_id, class: 'form-control' %>

    <%= f.submit "submit", class: "btn btn-primary" %>

    <% end %>
  </div>
